
class Node{
  Node izq, der;
  int dato;
  public Node(int d){
    dato = d;
  }
  public void insertar(int valor){
    if(valor <= dato){
     if(izq == null){
       izq = new Node(valor);
       }else{
        izq.insertar(valor);
        }
    }else{
      if(der == null){
        der = new Node(valor);
       }else{
        der.insertar(valor);
       }
     }
  }
  public Boolean buscar(int valor){
     if(valor == dato){
       return true;
     }else if(valor < dato){
       if(izq == null){
         return false
         }else{
           return izq.buscar(valor);
          }
     }else{
       if(der == null){
         return false;
         }else{
           return der.buscar(valor);
       }
     }
  }
}





